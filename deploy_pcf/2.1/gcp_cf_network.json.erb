<% cf_azs=`terraform output azs`.strip.split(",\n") 
ert_subnet_name=`terraform output ert_subnet_name`.strip %>
{
  "singleton_availability_zone": {
    "name": "<%= cf_azs[1] %>"
  },
  "other_availability_zones": [
    { "name": "<%= cf_azs[0] %>" },
    { "name": "<%= cf_azs[1] %>" },
    { "name": "<%= cf_azs[2] %>" }
  ],
  "network": {
    "name": "<%= ert_subnet_name %>"
  }
}
