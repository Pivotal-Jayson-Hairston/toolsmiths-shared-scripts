<% tcp_router=`terraform output tcp_router_pool`.strip
 http_lb=`terraform output http_lb_backend_name`.strip
 ws_router=`terraform output ws_router_pool`.strip
 ssh_router=`terraform output ssh_router_pool`.strip %>

{
  "tcp_router": {
    "elb_names": [
      "tcp:<%= tcp_router %>"
    ]
  },
  "router": {
    "elb_names": [
      "http:<%= http_lb %>",
      "tcp:<%= ws_router %>"
    ]
  },
  "diego_brain": {
    "elb_names": [
      "tcp:<%= ssh_router %>"
    ]
  }
}
